{
  "name": "govaxine",
  "version": "1.0.0",
  "description": "track nearby vaccine centre",
  "main": "server.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss public/css/style.css -w",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss public/css/style.comp.css",
    "prefix:sass": "postcss --use autoprefixer -b 'last 10 versions' public/css/style.comp.css -o public/css/style.prefix.css",
    "compress:sass": "node-sass public/css/style.prefix.css public/css/style.css --output-style compressed",
    "bd:sass": "npm-run-all compile:sass prefix:sass compress:sass"
  },
  "author": "vishnu prakash b",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.3.1",
    "concat": "^1.0.3",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5",
    "postcss-cli": "^8.3.1",
    "regenerator-runtime": "^0.13.9"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^0.22.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "corejs": "^1.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.1.0",
    "express-rate-limit": "^5.4.1",
    "hpp": "^0.2.3",
    "html-to-text": "^8.0.0",
    "jsonwebtoken": "^8.5.1",
    "leaflet": "^1.7.1",
    "me": "^0.3.0",
    "messagebird": "^3.7.1",
    "moment": "^2.29.1",
    "mongoose": "^6.0.9",
    "morgan": "^1.10.0",
    "node-schedule": "^2.0.0",
    "nodemailer": "^6.6.5",
    "npm": "^7.24.1",
    "pug": "^3.0.2",
    "validator": "^13.6.0",
    "xss-clean": "^0.1.1"
  }
}
